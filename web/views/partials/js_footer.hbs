<script type="text/javascript">
	$(function () {
			var $section = $('section');
			$(window).on("resize", function () {
					var dif = Math.max($(window).height() - $section.height(), 0);
					var padding = Math.floor(dif / 2) + 'px';
					$section.css({ 'padding-top': padding, 'padding-bottom': padding });
			}).trigger("resize");
			$('#stampedeToState').mbComingsoon({ expiryDate: new Date(2020, 1, 20, 00, 00, 00), speed:100 });
			setTimeout(function () {
					$(window).resize();
			}, 200);
	});
</script>
