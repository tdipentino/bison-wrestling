<div class="container" style="text-align: center;">
   <h1 class="mainH1">2018 - 2019 BISON</h1>
   <img src='img/Bison-Depth.png' style="max-width: 100%;" alt="Senior Night"/>
   <br>
   <br>
   
   <div class="mainH2" style="text-align:center; box-shadow: 5px 5px 0px #a9a9a9; margin-bottom:10px; border: #0c1c3c 1px solid;">Varsity</div>
   <br>
   <div class="row">
      {{#each components.bison}}
      {{#if (eq team "v")}}
      <div class="col-md-4" style="display:flex;justify-content:center;align-items:center;" id="{{lastname}}">
         <div class="grid mask">
            <p>
               {{#if (eq img_dir "y")}}
               <a href="#{{firstname}}{{lastname}}" data-toggle="modal" data-target="#{{firstname}}{{lastname}}">
               <img class="img-responsive" src="img/bison/{{firstname}}{{lastname}}.jpg" alt="{{firstname}} {{lastname}}" style="width:100%; height:100%;" />
               </a>
               {{else}}
               <a href="#{{firstname}}{{lastname}}" data-toggle="modal" data-target="#{{firstname}}{{lastname}}">
               <img class="img-responsive" src="//lh3.googleusercontent.com/SAx6ywdmNbbGs9vvhDeHrcabgOM5S22QiZuCpYfFHQAVnGQvr7AbwW8mo1otTUxMaFajguMyZ4xgA3w43INvSGIaFfH-J8TJ_bVricR0HRyVoei7ahjSvz3o-gDlydc87F8-2DuElA" alt="{{firstname}} {{lastname}}" style="width:100%; height:100%;" />
               </a>
               {{/if}}
            </p>
            <div class="mainH4">{{#if (eq is_captain 1)}}<img src="img/icons/captain.png" alt="Captain" /> {{/if}}{{firstname}} {{lastname}}</div>
            <div class="mainH4" style="margin-bottom:8%;">
               {{weight}} lbs - {{#if (eq grade 12)}}Senior{{/if}}{{#if (eq grade 11)}}Junior{{/if}}{{#if (eq grade 10)}}Sophomore{{/if}}{{#if (eq grade 9)}}Freshman{{/if}}
            </div>
         </div>
      </div>
      <!-- The Modal -->
      <div class="modal fade bd-modal-lg" id="{{firstname}}{{lastname}}" tabindex="-1" role="dialog" aria-labelledby="{{firstname}}{{lastname}}" aria-hidden="true">
         <div class="modal-dialog modal-lg">
            <div class="modal-content">
               <!-- Modal Header -->
               <div class="modal-header">
                  <h2 class="mainH2 modal-title" id="{{firstname}}{{lastname}}">{{#if (eq is_captain 1)}}<img src="img/icons/captain.png" alt="Captain" /> {{/if}} {{firstname}} {{lastname}}</h2>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                  </button>
               </div>
               <!-- Modal body -->
               <div class="modal-body">
                  {{#if (eq img_dir "y")}}
                  <img class="img-responsive" src="img/bison/{{firstname}}{{lastname}}.jpg" alt="{{firstname}} {{lastname}}" style="width:100%; height:100%;" />
                  {{else}}
                  <img class="img-responsive" src="//lh3.googleusercontent.com/SAx6ywdmNbbGs9vvhDeHrcabgOM5S22QiZuCpYfFHQAVnGQvr7AbwW8mo1otTUxMaFajguMyZ4xgA3w43INvSGIaFfH-J8TJ_bVricR0HRyVoei7ahjSvz3o-gDlydc87F8-2DuElA" alt="{{firstname}} {{lastname}}" style="width:100%; height:100%;" />
                  {{/if}}
                  <hr class="style13">
                  <table style="width: 100%;">
                     <tr>
                        <td class="mainH2" style="text-align:left; width: 50%">Grade:</td>
                        <td class="mainH2" style="text-align:right; width: 50%">{{#if (eq grade 12)}}Senior{{/if}}{{#if (eq grade 11)}}Junior{{/if}}{{#if (eq grade 10)}}Sophomore{{/if}}{{#if (eq grade 9)}}Freshman{{/if}}</td>
                     </tr>
                     <tr>
                        <td class="mainH2" style="text-align:left; width: 50%">Weight Class:</td>
                        <td class="mainH2" style="text-align:right; width: 50%">{{weight}} lbs</td>
                     </tr>
                     <tr>
                        <td class="mainH2" style="text-align:left; width: 50%">Years With Program:</td>
                        <td class="mainH2" style="text-align:right; width: 50%">{{program_years}}</td>
                     </tr>
                  </table>
               </div>
               <!-- Modal footer -->
               <div class="modal-footer">
                  <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
               </div>
            </div>
         </div>
      </div>
      {{/if}}
      {{/each}}
   </div>
   
